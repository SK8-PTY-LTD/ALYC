<div class="container">
	<div class="row">
		<div class="col-md-12">
			<h1>活动相册</h1>
			<br><br>
		</div>
	</div>
	<div class="row" ng-controller="GalleryCtrl">
		<div class="form-group">
			<div class="col-md-3 img-thumbnail" ng-repeat="photo in photos">
				<img ng-src="{{ photo.get('file').url() }}" class="col-xs-12" alt="">
			</div>
		</div>
	</div>

	<div ng-controller="GalleryCtrl">
		<div class="Collage">
			<div ng-repeat="photo in photos">
				<img Collage ng-src="{{ photo.get('file').url() }}" alt="">
			</div>

		</div>
	</div>

</div>

<script>
	$('.Collage').removeWhitespace().collagePlus();

</script>